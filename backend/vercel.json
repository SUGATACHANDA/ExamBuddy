{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/exams/countdown/.*",
      "dest": "/api/server.js"
    },
    {
      "src": "/socket.io/",
      "dest": "/api/socket.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/server.js"
    }
  ],
  "functions": {
    "api/server.js": {
      "includeFiles": "backend/assets/font/**"
    }
  }
}
